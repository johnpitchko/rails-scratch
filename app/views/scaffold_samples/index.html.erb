<p id="notice"><%= notice %></p>

<h1>Scaffold Samples</h1>

<table>
  <thead>
    <tr>
      <th>Some text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scaffold_samples.each do |scaffold_sample| %>
      <tr>
        <td><%= scaffold_sample.some_text %></td>
        <td><%= link_to 'Show', scaffold_sample %></td>
        <td><%= link_to 'Edit', edit_scaffold_sample_path(scaffold_sample) %></td>
        <td><%= link_to 'Destroy', scaffold_sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scaffold Sample', new_scaffold_sample_path %>
